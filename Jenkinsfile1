pipeline {
    agent any

    options {
        timeout(time: 60, unit: 'MINUTES')
        buildDiscarder(logRotator(numToKeepStr: '5'))
        timestamps()
    }

    parameters {
        string(
            name: 'Job_name',
            description: 'Job_name treba da bude isto kao i service_name odnosno ime docker kontejnera koji treba da bude izbildovan.')
    }


    stages {
        stage('Test') {
            steps {
                script {
                    echo "Cao Cao"
                }
            }
        }
    }

}
